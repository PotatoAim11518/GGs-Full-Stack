extends layout

append head
    //- add page specific styles by appending to the head
    link(rel="stylesheet" href="/stylesheets/allPublishers.css")
    //- add page specific js
    script(src="/javascripts/index.js" type="module" defer)
block content
    div.allGameBox
        h2.allGamesHeader All Publishers
        each publisher in publishers
            div
                a(href=`${publisher.id}`) #{publisher.publisherName}